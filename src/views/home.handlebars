<title>Lista de Productos</title>

<body>
  <h1>Lista de Productos</h1>

  <table class="table table-hover">
    <thead>
      <tr>
        <th>Nombre</th>
        <th>Descripci√≥n</th>
        <th>Precio</th>
        <th>Stock</th>
        <th>Imagen</th>
      </tr>
    </thead>



     <tbody>
  {{#each productsPlain}}
    <tr>
      <td>{{title}}</td>
      <td>{{price}}</td>
